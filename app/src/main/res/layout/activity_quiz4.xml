<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.tanishka.cpro.Quiz4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
   >
    <TextView
        android:layout_width="match_parent"
        android:textColor="#ff0359"
        android:layout_height="wrap_content"
        android:text="What is right way to Initialize array?"/>
    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onclickRadio41"
            android:id="@+id/a41"
            android:text="int num[6] = { 2, 4, 12, 5, 45, 5 };"/>

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="int n{} = { 2, 4, 12, 5, 45, 5 };"
            android:onClick="onclickRadio41"
            android:id="@+id/b41"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="int n{6} = { 2, 4, 12 };"
            android:onClick="onclickRadio41"
            android:id="@+id/c41"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="int n(6) = { 2, 4, 12, 5, 45, 5 };"
            android:onClick="onclickRadio41"
            android:id="@+id/d41"/>

    </RadioGroup>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#ff0359"
        android:text="\nWhat will be the output of the program ?
\n#include&lt;stdio.h&gt;
\nvoid main()
\n{
    \nint a[5] = {5, 1, 15, 20, 25};
    \nint i, j, m;
    \ni = ++a[1];
    \nj = a[1]++;
    \nm = a[i++];
    \nprintf(&quot;%d, %d, %d&quot;, i, j, m);
    \n}"/>
    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onclickRadio42"
            android:id="@+id/a42"
            android:text="3, 2, 15"/>

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2, 3, 20"
            android:onClick="onclickRadio42"
            android:id="@+id/b42"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2, 1, 15"
            android:onClick="onclickRadio42"
            android:id="@+id/c42"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1, 2, 5"
            android:onClick="onclickRadio42"
            android:id="@+id/d42"/>

    </RadioGroup>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#ff0359"
        android:text="\nAn array elements are always stored in ________ memory locations."/>
    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onclickRadio43"
            android:id="@+id/a43"
            android:text="Sequential"/>

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Random"
            android:onClick="onclickRadio43"
            android:id="@+id/b43"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sequential and Random"
            android:onClick="onclickRadio43"
            android:id="@+id/c43"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="None of the above"
            android:onClick="onclickRadio43"
            android:id="@+id/d43"/>

    </RadioGroup>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="Continue"
        android:text="Continue"/>
</LinearLayout>
</ScrollView>